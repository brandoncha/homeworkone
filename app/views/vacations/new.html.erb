<div class="booyah-box col-xs-10 col-xs-offset-1">
<h1>Enter Your Vacation Details</h1>
<div class="clear"></div>
	<%= simple_form_for @vacation do |f| %>
		<%= f.input :name %>
		<%= f.input :address %>
		<%= f.input :description %>
		<br/>
		<%= f.submit 'Create', class:'btn btn-primary' %>
	<% end %>
</div>